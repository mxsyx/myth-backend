  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["ES2020", "DOM", "DOM.Iterable", "Decorators"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
